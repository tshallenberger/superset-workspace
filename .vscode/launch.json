  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Superset",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "superset.app:create_app()",
        "FLASK_DEBUG": "1",
        "PYTHONPATH": "/workspace/config"
      },
      "args": [
        "run",
        "--port",
        "8088",
        "--no-debugger",
        "--no-reload"
      ],
      "jinja": true,
      "justMyCode": false
    }
  ]
}